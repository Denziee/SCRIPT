toolname = "Light Weight", "Squat", "Meditate,"

_G.Enabled = false

while _G.Enabled do
pcall(function()
game.Players.LocalPlayer.Character.Humanoid:EquipTool(game:GetService("Players").LocalPlayer.Backpack:FindFirstChildOfClass(toolname))

local Event = game:GetService("ReplicatedStorage").Events.Rebirth
Event:InvokeServer()

local Event = game:GetService("ReplicatedStorage").Events.bar
Event:InvokeServer()

local Event = game:GetService("ReplicatedStorage").Events.Stomp
Event:InvokeServer()

local Event = game:GetService("ReplicatedStorage").Events.train
Event:InvokeServer()

local Event = game:GetService("ReplicatedStorage").Events.str
Event:InvokeServer()

local Event = game:GetService("ReplicatedStorage").Events.leg
Event:InvokeServer()

local Event = game:GetService("ReplicatedStorage").Events.Med
Event:InvokeServer()
end)
end
