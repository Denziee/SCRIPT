toolname = "Light Weight"
_G.Enabled = true
 
while _G.Enabled do
wait()
pcall(function()
game.Players.LocalPlayer.Character.Humanoid:EquipTool(game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(toolname))


local Event = game:GetService("ReplicatedStorage").Events.str:InvokeServer()
end)
end
